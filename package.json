{
  "name": "raw-githack-button",
  "version": "0.1.0",
  "description": "A userscript to add raw.githack.com button on Github.",
  "scripts": {
    "test": "eslint **/*.js",
    "preversion": "npm test",
    "version": "userscript-meta -u raw-githack-button.user.js && git add .",
    "postversion": "git push --follow-tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eight04/raw-githack-button.git"
  },
  "author": "eight <eight04@gmail.com> (https://github.com/eight04)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eight04/raw-githack-button/issues"
  },
  "homepage": "https://github.com/eight04/raw-githack-button#readme",
  "devDependencies": {
    "eslint": "^5.13.0",
    "userscript-meta-cli": "^0.4.2"
  },
  "userscript": {
    "name": "Github raw.githack.com Button",
    "namespace": "https://github.com/eight04",
    "include": [
      "https://github.com/*",
      "https://gist.github.com/*"
    ]
  }
}
